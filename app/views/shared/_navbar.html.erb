<div class="left">
      <div class="logo">
        <%= image_tag('logo.png', width: '50px') %>
      </div>
      <ul class="left_list">
        <li>
          <i class='bx bx-home-circle'></i>
          <%= link_to "Home", dashboard_path, style: 'text-decoration:none;color:#fff' %>
        </li>
        <li>
          <i class='bx bx-toggle-right'></i>
          Explore
        </li>
        <li>
          <i class='bx bx-bell' ></i>
          Notifications
        </li>
        <li>
          <i class='bx bx-message-rounded' ></i>
          Message
        </li>
        <li>
          <i class='bx bx-bookmarks' ></i>
          Bookmarks
        </li>
        <li>
          <i class='bx bx-user' ></i>
          <%= link_to "Profile", user_path(current_user), style: 'text-decoration:none;color:#fff' %>
        </li>
        <li>
          <i class='bx bx-list-ul' ></i>
          Lists
        </li>
        <li>
          <i class='bx bx-dots-horizontal-rounded' ></i>
          More
        </li>
      </ul>

      <label for="ip-logout" style="display: flex;align-items: center;top: 130px;position: relative;padding: 10px;cursor:pointer;border-radius:14px">
        <div style="display: flex;align-items: center;">
          <% if current_user.image.present? %>
            <%= image_tag url_for(current_user.image), style: 'width: 40px;height:40px;border-radius:50%'%>
          <% else %>
            <img src="https://static.vecteezy.com/system/resources/thumbnails/009/292/244/small/default-avatar-icon-of-social-media-user-vector.jpg" width='40px' style='border-radius:50%'/>
          <% end %>
        </div>
        <div style="margin-left: 10px;">
          <p style="margin: 0"><%= current_user.name %></p>
          <p style="margin: 0"><%= current_user.nickname %></p>
        </div>
      </label>

      <input type="checkbox" hidden id="ip-logout" class="ip"/>

      <div class="h4_logout" style="position: absolute;border: 1px solid #ccc;border-radius: 14px;left: 54px;display:none">
        <%= link_to "Logout", logout_path %>
      </div>
    </div>
